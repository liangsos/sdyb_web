/*CheckBox特殊处理*/
.modal-panel input[type=checkbox][name=checkbox-rjjh] {
    display: none;
}

.modal-panel input[type=checkbox] + span {
    background-color: #fff;
    border: 1px solid #c6d4e7;
    width: 14px;
    height: 14px;
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    line-height: 12px;
}

.modal-panel input[type=checkbox]:checked + span {
    background-color: #4ea0f5;
    border: solid 1px #4ea0f5;
}

    .modal-panel input[type=checkbox]:checked + span:after {
        content: "\2714";
        color: #fff;
    }

.modal-panel input[type=checkbox]:disabled + span {
    background-color: #F5F5F5;
    cursor: not-allowed;
}

/*Radio特殊处理*/
.modal-panel input[type=radio] {
    display: none;
}

    .modal-panel input[type=radio]:disabled {
        cursor: not-allowed;
    }

    .modal-panel input[type=radio] + span {
        -webkit-appearance: none;
        background-color: #fff;
        border: 1px solid #c6d4e7;
        width: 16px;
        height: 16px;
        border-radius: 50%;
        display: inline-block;
        position: relative;
        vertical-align: middle
    }

    .modal-panel input[type=radio]:checked + span:after {
        position: absolute;
        top: 3px;
        left: 3px;
        content: "";
        width: 8px;
        height: 8px;
        border-radius: 50%;
        background: #4ea0f5;
    }

