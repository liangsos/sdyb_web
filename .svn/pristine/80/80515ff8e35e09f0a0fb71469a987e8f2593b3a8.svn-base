.loading-layer-home-select-tm {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 9999;
    background: rgba(255,255,255,0.3);
}

.loading-content-home-select-tm {
    position: relative;
    width: 380px;
    height: auto;
    background: #f2f5f7;
    margin: 19% auto;
    text-align: center;
    vertical-align: middle;
    border-radius: 5px;
    border: solid 1px #cadde9;
    box-shadow: 2px 2px 2px #eeeeee;
    padding: 2px 8px 8px 8px;
    font-size: 14px;
    color: #0d79ef;
}



    .loading-content-home-select-tm .loading-layer-close {
        display: block;
        position: absolute;
        top: 3px;
        right: 3px;
        width: 20px;
        height: 20px;
        background: url(../static/image/img/icon_load_close.png) no-repeat;
        cursor: pointer;
    }

